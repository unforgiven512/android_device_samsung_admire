# mount     fstype  device                  [device2]         [fstype2]   [fsoptions]     [fsoptions2]

/boot       bml     boot                    NULL                NULL        NULL            NULL
/recovery   bml     recoveryonly            NULL                NULL        NULL            NULL
/cache      ext4    /dev/block/stl14        NULL                rfs         NULL            rw,nosuid,nodev,check=no
/data       ext4    /dev/block/stl13        NULL                rfs         NULL            rw,nosuid,nodev,check=no
/sdcard     vfat    /dev/block/mmcblk0p1    /dev/block/mmcblk0  NULL        NULL            NULL
/sd-ext     ext4    /dev/block/mmcblk0p2    NULL                NULL        NULL            NULL
/system     ext4    /dev/block/stl12        NULL                rfs         NULL            check=no
